<Window x:Class="DistributedServicesCW.Login"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Login" Height="300" Width="300">
    <Grid>
        <Button x:Name="lblBack" Content="Back" HorizontalAlignment="Left" Margin="38,204,0,0" VerticalAlignment="Top" Width="75" Click="lblBack_Click"/>
        <Button x:Name="btnSignIn" Content="Sign In" HorizontalAlignment="Left" Margin="164,204,0,0" VerticalAlignment="Top" Width="75" Click="btnSignIn_Click"/>
        <TextBox x:Name="txtUsername" HorizontalAlignment="Left" Height="23" Margin="119,38,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="lblUsername" Content="Username" HorizontalAlignment="Left" Margin="38,35,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtPassword" HorizontalAlignment="Left" Height="23" Margin="119,112,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="lblPassword" Content="Password" HorizontalAlignment="Left" Margin="38,109,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
